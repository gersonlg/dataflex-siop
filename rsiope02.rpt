                                                        IF ((MOVDES.TIPO_DESPESA EQ 'OR') AND (MOVDES.COD_INSTRUM EQ '')) BEGIN
                                                         GRAVA_TEMPCALC ST_CHAVE_ELEMENTO 'MOVDES' MOVDES.FIXADO_INICIAL '' ST_TIPO_DESPESA 'FI'
                                                        END
