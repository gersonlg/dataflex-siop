//               [ OK!] INDICATE OK! AS ST_FUNCAO        LE JN_N_FUNCAO_FINAL
//               [ OK!] INDICATE OK! AS ST_SUBFUNCAO     GE JN_N_SUBFUNCAO_INICIAL
