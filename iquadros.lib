//--- indica entidades gestoras de previdencia 
MOVEALL '' TO ST@RREO@GESTOR ST@RREO@ENT@SAUDE
TRIM ST@RREO@GESTOR  TO ST@RREO@GESTOR
CLEAR ENTIDADE
MOVE ST@EXERCICIO@FINANCEIRO TO ENTIDADE.EXERCICIO
REPEAT
 FIND GT ENTIDADE BY INDEX.1
 [ FOUND] INDICATE FOUND AS ENTIDADE.EXERCICIO EQ ST@EXERCICIO@FINANCEIRO
 [ FOUND] BEGIN
           IF ENTIDADE.TP_ENTIDADE EQ '1' BEGIN
            IF ST@RREO@GESTOR EQ '' BEGIN
             MOVE ENTIDADE.CODIGO TO ST@RREO@GESTOR
             TRIM ST@RREO@GESTOR  TO ST@RREO@GESTOR
            END
            ELSE BEGIN
             APPEND ST@RREO@GESTOR '#' ENTIDADE.CODIGO 
            END
           END
           ELSE IF ENTIDADE.TP_ENTIDADE EQ '2' BEGIN
            MOVE ENTIDADE.CODIGO TO ST@RREO@ENT@SAUDE
           END
           INDICATE FOUND TRUE
          END 
UNTIL [~FOUND]

//--- define versao de demonstrativos RREO e RGF: Homologacao e Oficial(Cliente)
#COMMAND VERIFICA_VERSAO_RREO
 #IFDEF     IT@RREO@VERSAO
 #ELSE
    INTEGER IT@RREO@VERSAO
 #ENDIF
 #IFDEF     IT@RREO@VERSAO@OFICIAL
 #ELSE
    INTEGER IT@RREO@VERSAO@OFICIAL
 #ENDIF
 #IFDEF     IT@RREO@EXERCICIO
 #ELSE
    INTEGER IT@RREO@EXERCICIO
 #ENDIF
 #IFDEF     ST@RREO@TIPO
 #ELSE
    STRING  ST@RREO@TIPO  6
 #ENDIF
 #IFDEF     ST@RREO@VERSAO
 #ELSE
    STRING  ST@RREO@VERSAO  45
 #ENDIF
  #IFDEF    ST@RREO@VERSAO@HOMOLOGACAO
 #ELSE
    STRING  ST@RREO@VERSAO@HOMOLOGACAO 12
 #ENDIF
 #IFDEF     IT@RREO@ANEXO
 #ELSE
    INTEGER IT@RREO@ANEXO
 #ENDIF
 #IFDEF     DT@RREO@DATA
 #ELSE
    DATE    DT@RREO@DATA
 #ENDIF
 MOVEALL '' TO ST@RREO@TIPO ST@RREO@VERSAO ST@RREO@VERSAO@HOMOLOGACAO
 MOVEALL 0  TO IT@RREO@VERSAO IT@RREO@VERSAO@OFICIAL IT@RREO@EXERCICIO IT@RREO@ANEXO
 
 MOVE !1      TO ST@RREO@TIPO
 MOVE !2      TO IT@RREO@ANEXO
 MOVE !3      TO IT@RREO@EXERCICIO
 MOVE !5      TO DT@RREO@DATA
 MOVE 'CONAM' TO ST@RREO@VERSAO
 
 IF PARGERAL.EXERCICIO EQ '2022' BEGIN
  //--- 1.1.001(04/03/2022) -- 16/03/2022 -- correcoes apos homologacao  -- 1.5.001 (25/03/2022) -- 1.5.005 (29/03/2022) -- 1.6.001 (08/04/2022) -- 1.7.001 (14/04/2022) -- 1.8.001 (11/05/2022) 
  //--- 1.9.004(27/05/2022) -- valores para o simplificado  -- 1.9.005(31/05/2022) -- valores de elemento 91 - quadro IV -- 1.9.006 (01/06/2022) -- LINHA 45 - estava entrando outras funcoes, alem da 12.
  //--- 2o.bimestre oficial - 1.9.002 (25/05/2022)
  //--- 3o.bimestre oficial - 1.9.008 (c¢d.aplicacao - 313)
  //--- 4o.bimestre oficial - 2.1.001 (11/10/2022) 
  MOVE '2'    TO IT@RREO@VERSAO             
  MOVE '1'    TO IT@RREO@VERSAO@OFICIAL
  MOVE '001'  TO ST@RREO@VERSAO@HOMOLOGACAO
  IF ST@RREO@TIPO  EQ 'RREO' BEGIN
   IF ((IT@RREO@ANEXO EQ 8) OR (IT@RREO@ANEXO EQ 12)) BEGIN
    //--- 2.1.002 (01/11/2022) - 
    //--- anexo  8 - alteracao para quadro 34 - valores de covid - solicitacao Ilson/consultoria
    //--- anexo 12 - alteracao para CNR  1.7.1.1.51.2/3 - cota-parte FPM
    //--- anexo  8 - alteracao para CNR  1.7.2.9.99.0.1 - FR/CA: 01.200 ensino - FR/CA: 01.310 saude (17/01/2023)
    MOVE '004'  TO ST@RREO@VERSAO@HOMOLOGACAO
   END  
   IF ((IT@RREO@ANEXO EQ 9) OR (IT@RREO@ANEXO EQ 11)) BEGIN
    MOVE '2'    TO IT@RREO@VERSAO@OFICIAL
    MOVE '001'  TO ST@RREO@VERSAO@HOMOLOGACAO
   END
   //--- atualizacao encerramento do exercicio (23/01/2023)
   MOVE  '2'    TO IT@RREO@VERSAO             
   MOVE  '2'    TO IT@RREO@VERSAO@OFICIAL
   MOVE  '004'  TO ST@RREO@VERSAO@HOMOLOGACAO
  END
  IF ST@RREO@TIPO  EQ 'RGF' BEGIN
   IF IT@RREO@ANEXO EQ 5 BEGIN
    //--- 2.1.002 (29/11/2022) - 
    MOVE '2'    TO IT@RREO@VERSAO@OFICIAL
    MOVE '001'  TO ST@RREO@VERSAO@HOMOLOGACAO
   END
   //--- atualizacao encerramento do exercicio (23/01/2023)
   MOVE  '2'    TO IT@RREO@VERSAO             
   MOVE  '2'    TO IT@RREO@VERSAO@OFICIAL
   MOVE  '004'  TO ST@RREO@VERSAO@HOMOLOGACAO
  END
  IF ((ST@RREO@TIPO EQ 'ENSINO') OR (ST@RREO@TIPO EQ 'SAUDE')) BEGIN
   MOVE '2'    TO IT@RREO@VERSAO             
   MOVE '1'    TO IT@RREO@VERSAO@OFICIAL
   MOVE '003'  TO ST@RREO@VERSAO@HOMOLOGACAO
   //--- atualizacao encerramento do exercicio (23/01/2023)
   MOVE '2'    TO IT@RREO@VERSAO             
   MOVE '2'    TO IT@RREO@VERSAO@OFICIAL
   MOVE '004'  TO ST@RREO@VERSAO@HOMOLOGACAO
  END 
  APPEND ST@RREO@VERSAO '-' (TRIM(ST@RREO@TIPO)) IT@RREO@ANEXO '-' IT@RREO@EXERCICIO '-' IT@RREO@VERSAO '.' IT@RREO@VERSAO@OFICIAL 
  TRIM ST@RREO@VERSAO TO ST@RREO@VERSAO
  IF ST@USUARIO IN 'CONAM#NOSSA' BEGIN
   APPEND ST@RREO@VERSAO '.' ST@RREO@VERSAO@HOMOLOGACAO
  END 
 END 
 IF PARGERAL.EXERCICIO GE '2023' BEGIN
  APPEND ST@RREO@VERSAO '-' (TRIM(ST@RREO@TIPO)) IT@RREO@ANEXO '-' IT@RREO@EXERCICIO
  TRIM ST@RREO@VERSAO TO ST@RREO@VERSAO
 END 
#ENDCOMMAND

