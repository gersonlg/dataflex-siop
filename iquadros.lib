//--- LER_RECEITAS <codigo da receita> <destino da descricao da receita> <espacos no inicio> <'S' para minusculos>
#COMMAND LER_RECEITAS
 CLEAR RECEITAS
 MOVE PARGERAL.ESTADO    TO RECEITAS.UF
 MOVE PARGERAL.EXERCICIO TO RECEITAS.ANO
 MOVE !1                 TO RECEITAS.CODIGO
 FIND EQ RECEITAS BY INDEX.1
 MOVE RECEITAS.DESCRICAO TO !2
 INSERT ' ' IN !2 AT 1
 IF !4 EQ 'S' LOWERCASE !2
 INSERT !3 IN !2 AT 1
#ENDCOMMAND

//--- LER_ECONOMICAS <codigo da economica> <destino da descricao da economica> <espacos no inicio> <'S' para minusculos>
#COMMAND LER_ECONOMICAS
 BLANKFORM BODY_DESPESA
 CLEAR ECONORCA
 MOVE PARGERAL.ESTADO    TO ECONORCA.UF
 MOVE PARGERAL.EXERCICIO TO ECONORCA.ANO
 MOVE !1                 TO ECONORCA.CODIGO
 FIND EQ ECONORCA BY INDEX.1
 IF (LEFT(ECONORCA.CODIGO,1)) EQ '9' BEGIN
  MOVE 'RESERVA DE CONTINGENCIA' TO !2
 END
 ELSE MOVE ECONORCA.DESCRICAO TO !2
 INSERT ' ' IN !2 AT 1
 IF !4 EQ 'S' LOWERCASE !2
 INSERT !3 IN !2 AT 1
#ENDCOMMAND

