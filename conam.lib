        ELSE IF IT@GRAU EQ '7' APPEND ST@CODIGO@ANTERIOR (MID(ST@CODIGO@RECEITA,12,1)) '.0.0000'
        ELSE IF IT@GRAU EQ '6' APPEND ST@CODIGO@ANTERIOR (MID(ST@CODIGO@RECEITA,10,1)) '.0.0.0000'
