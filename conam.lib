#COMMAND VERIFICA_ENTIDADE_IMPORTACAO
#IFDEF    IT@RECNUM@PARGERAL
#ELSE
  INTEGER IT@RECNUM@PARGERAL
#ENDIF
#IFDEF    IT@MSG
#ELSE
  INTEGER IT@MSG
#ENDIF
 
