#COMMAND ACCEPT_D
   #IFDEF IT@DATA
   #ELSE
        INTEGER IT@DATA
   #ENDIF
    INDICATE DATA_ERRADA! FALSE
    ACCEPT !1 {CAPSLOCK,AUTOCLEAR}
    LENGTH !1 TO IT@DATA
    IF IT@DATA NE '';
      IF IT@DATA LT 10 BEGIN
                          IF !1 GE 01/01/10 CALC (!1 + 693975) TO !1
                          ELSE              CALC (!1 + 730500) TO !1
                         END
