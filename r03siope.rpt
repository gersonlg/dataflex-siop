NAME JN_N_FUNCAO            JN_FUNCAO
NAME JN_N_SUBFUNCAO_INICIAL JN_SUBFUNCAO_INICIAL
NAME JN_N_SUBFUNCAO_FINAL   JN_SUBFUNCAO_FINAL
NAME JN_N_FONTE_INICIAL     JN_FONTE_INICIAL
NAME JN_N_FONTE_FINAL       JN_FONTE_FINAL
