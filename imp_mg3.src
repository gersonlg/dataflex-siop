#IFDEF    IT@POS@INICIO
#ELSE
   INTEGER IT@POS@INICIO
#ENDIF
#IFDEF    IT@POS@FINAL
#ELSE
   INTEGER IT@POS@FINAL
#ENDIF
#IFDEF    IT@AUX
#ELSE
