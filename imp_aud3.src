 [ @VALIDO!] BEGIN
              MOVEALL '' TO ST_ORGAO ST_UO ST_UE ST_FUNCAO ST_SUBFUNCAO ST_PROGRAMA ST_ACAO
              MOVEALL '' TO ST_ECONOMICA ST_FONTE ST_CODIGO_APLICACAO ST_DATA_MOVIMENTO ST_CONTA
              MOVEALL '' TO ST_AUXILIAR

              REPEAT
