//--conta corrente 08
 [~MES_14!] VALIDA_DADO '<AlteracaoDotacaoOrcamentaria>'    '<ctb:AlteracaoDotacaoOrcamentaria>'    '</AlteracaoDotacaoOrcamentaria>'    '</ctb:AlteracaoDotacaoOrcamentaria>'    ST_LINHA ST_NOME_CC_1 ST_NOME_CC_2
