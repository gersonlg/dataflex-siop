               PEGA_DADO '<gen:UO>'                        '</gen:UO>'                        ''                                    ''                                     ST_LINHA ST_UO
               PEGA_DADO '<gen:UE>'                        '</gen:UE>'                        ''                                    ''                                     ST_LINHA ST_UE
               
