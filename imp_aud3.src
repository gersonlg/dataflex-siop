                                  MOVE '            '            TO ST_REGIME_DESPESA
                                 END

              [ COD_APL_DIFERENTE!] BEGIN
                                     MONTA_COD_APLICACAO ST_CODIGO_APLICACAO ST_CODIGO_APLICACAO
                                    END

              COMZEROS ST_ORGAO        5
