#INCLUDE CONAM.LIB
STRING ST_DESTINO 255
INICIO:

INDICATE UBERLANDIA! AS PARGERAL.CIDADE EQ 'UBERLANDIA'

[~UBERLANDIA!] MOVE 'PLANDESP ' TO ST_DESTINO
[ UBERLANDIA!] MOVE 'PLANDEMG ' TO ST_DESTINO

REPEAT
 REPLACE ' ' IN ST@PREFEITURA WITH '@'
UNTIL [~FOUND]

APPEND ST_DESTINO ST@PREFEITURA ' ' ST@USUARIO ' ' ST@OPCAO ' ' ST@PROGRAMA ' ' ST@CODIGO@ENTIDADE ' ' ST@EXERCICIO@FINANCEIRO
CHAIN WAIT ST_DESTINO EXPORT_FILES
ABORT



